[myapp-up]
command=set -a; . .env; set +a && go run ./cmd/myapp
cwd=<root>
output=terminal
pos=konsole
save=2

[up-watch]
command=set -a; . .env; set +a && reflex -c reflex.conf
cwd=<root>
save=2
output=terminal

[myapp-build]
command=bash ./scripts/build.sh
cwd=<root>
output=terminal
pos=konsole
save=2
